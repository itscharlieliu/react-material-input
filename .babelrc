{
  "env": {
    "browser": {
      "presets": [
        ["babel-preset-env", {
          "targets": {
            "browsers": "last 2 versions, ie 10-11"
          },
          "modules": false
        }]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties"
      ]
    }
  },
  "sourceMaps": true
}